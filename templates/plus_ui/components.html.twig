{% extends 'base.html.twig' %}

{% block title %}Components - Plus UI Design System{% endblock %}

{% block stylesheets %}
    <link rel="stylesheet" href="{{ asset('css/components/component-card.css') }}">
{% endblock %}

{% block content %}
    <!-- Components Hero Section -->
    <section class="components-hero">
        <div class="container">
            <h1 class="components-hero__title">Components</h1>
            <p class="components-hero__description">
                A carefully crafted set of Figma UI components, thoughtfully designed to help you create 
                beautiful and intuitive interfaces efficiently. Whether you're building a product, dashboard, 
                or website, our UI kits provide you with reusable design elements.
            </p>
        </div>
    </section>

    <!-- Components Container -->
    <div class="components-container">
        <div class="container">
            <!-- Actions Section -->
            <section class="component-category" id="actions">
                <h2 class="component-category__title">Actions</h2>
                <div class="component-grid">
                    <!-- Button -->
                    <div class="component-card">
                        <div class="component-card__showcase">
                            <div class="showcase-buttons">
                                <button class="showcase-button showcase-button--primary">Button</button>
                                <button class="showcase-button showcase-button--secondary">Button</button>
                                <button class="showcase-button showcase-button--ghost">Button</button>
                                <button class="showcase-button showcase-button--disabled" disabled>Button</button>
                            </div>
                        </div>
                        <div class="component-card__divider"></div>
                        <div class="component-card__title-area">
                            <span class="component-card__name">Button</span>
                            <span class="chip">8 variants</span>
                        </div>
