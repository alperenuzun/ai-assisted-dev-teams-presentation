                        <div class="component-card__title-area">
                            <span class="component-card__name">Button Group</span>
                            <span class="chip">2 variants</span>
                        </div>
                    </div>

                    <!-- Link -->
                    <div class="component-card">
                        <div class="component-card__showcase">
                            <div class="showcase-links">
                                <a href="#" class="showcase-link">Link with icon ‚Üí</a>
                                <a href="#" class="showcase-link showcase-link--underline">Underline link</a>
                                <a href="#" class="showcase-link">External link ‚Üó</a>
