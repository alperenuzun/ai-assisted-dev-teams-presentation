{
    "preset": "laravel",
    "rules": {
        "declare_strict_types": true,
        "final_class": false,
        "no_unused_imports": true,
        "ordered_imports": {
            "sort_algorithm": "alpha"
        },
        "single_line_throw": false
    },
    "exclude": [
        "vendor",
        "var",
        "public",
        "migrations"
    ]
}
